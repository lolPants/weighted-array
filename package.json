{
  "name": "weighted-map",
  "version": "0.3.0",
  "description": "Select a random element from a Map<element, weight>",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "prepublishOnly": "npm run test && npm run build",
    "test": "npm run check && npm run lint && npm run ava",
    "ava": "nyc --reporter=html --reporter=text ava",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "check": "npm run build -- --noEmit",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prebuild": "rimraf lib",
    "build": "tsc --sourceMap",
    "watch": "tsc --watch",
    "dev": "npm run watch -- --sourceMap"
  },
  "repository": "git@github.com:lolPants/weighted-array.git",
  "author": "Jack Baron <jackmbaron@gmail.com>",
  "keywords": [
    "weighted",
    "random",
    "map",
    "weight",
    "weights"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lolPants/weighted-array/issues"
  },
  "homepage": "https://github.com/lolPants/weighted-array#readme",
  "devDependencies": {
    "@types/node": "^11.13.4",
    "ava": "^1.4.1",
    "coveralls": "^3.0.3",
    "nyc": "^13.3.0",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "ts-node": "^8.0.3",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.4.3"
  },
  "ava": {
    "compileEnhancements": false,
    "files": [
      "test/**/*.ts",
      "!test/helpers.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ]
  }
}
